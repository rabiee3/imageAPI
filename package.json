{
  "name": "imageapi",
  "version": "1.0.0",
  "description": "an api for resizing image",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint ./src/**/*.ts",
    "lint-fix": "eslint ./src/**/*.ts --fix",
    "lint-config-init": "eslint --init",
    "typerscript-config-init": "npx tsc --init",
    "jasmine": "jasmine",
    "serve": "npm run prettier && nodemon src/index.ts",
    "unit-test": "npm run build && npm run jasmine",
    "copy-full-image": "copyfiles -u 2 src/full/*.png full",
    "build": "npm run prettier && npx tsc && npm run copy-full-image",
    "build:prod": "npx tsc && SET NODE_ENV=production",
    "start": "npm run build && SET NODE_ENV=development && node dist/."
  },
  "author": "Rabie Mohamed",
  "license": "ISC",
  "devDependencies": {
    "@types/canvasjs": "^1.9.7",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^16.11.12",
    "@types/sharp": "^0.29.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "jasmine": "^3.10.0",
    "nodemon": "^2.0.15",
    "prettier": "^1.19.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "canvas": "^2.8.0",
    "copyfiles": "^2.4.1",
    "express": "^4.17.1",
    "jasmine-spec-reporter": "^7.0.0",
    "loadash": "^1.0.0",
    "sharp": "^0.28.3",
    "supertest": "^6.1.6"
  }
}
